{
  "name": "__MSG_appName__",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "default_locale": "en",
  "background": {
    "persistent": false,
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "css": [
        "styles/contentstyle.css"
      ]
    }
  ],
  "permissions": [
    "https://github.com/*/*",
    "webNavigation"
  ]
}